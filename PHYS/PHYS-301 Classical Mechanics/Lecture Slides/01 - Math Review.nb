     "                                                            ", 
     RowBox[{"=", " ", 
      RowBox[{
       UnderscriptBox["\[Sum]", 
        RowBox[{"exterior", " ", "surfaces"}]], 
       RowBox[{
        RowBox[{"(", " ", 
         RowBox[{
          OverscriptBox["n", "^"], " ", "\[CenterDot]", " ", 
          OverscriptBox["V", "\[RightVector]"]}], ")"}], " ", 
        SuperscriptBox["d", "2"], 
        RowBox[{"r", "           ", "."}]}]}]}]}],
    TraditionalForm]}], TraditionalForm]], "DisplayFormulaNumbered",
 CellChangeTimes->{{3.430345683539258*^9, 3.430345721772069*^9}, 
   3.77443853810756*^9, {3.7744405457997503`*^9, 

Cell[BoxData[
 FormBox[
  RowBox[{"             ", 
   RowBox[{
    FormBox[
     FrameBox[
      FormBox[
       RowBox[{
        RowBox[{
         SubscriptBox["\[Integral]", 
          RowBox[{"Volume", " ", "\[CapitalOmega]"}]], 
         RowBox[{
          SuperscriptBox["d", "3"], "r", "  ", 
          RowBox[{"\[Del]", " ", 
           RowBox[{"\[CenterDot]", " ", 
            RowBox[{
             OverscriptBox["V", "\[RightVector]"], "(", " ", 
             OverscriptBox["r", "\[RightVector]"], " ", ")"}]}]}]}]}], " ", 
        "=", " ", 
        RowBox[{
         SubscriptBox["\[Integral]", 
          RowBox[{"Surface", " ", "S"}]], " ", 
         RowBox[{
          SuperscriptBox["d", "2"], "r", "   ", 
          RowBox[{
           OverscriptBox["n", "^"], " ", "\[CenterDot]", " ", 
           OverscriptBox["V", "\[RightVector]"]}]}]}]}],
       TraditionalForm],
      FrameMargins->{{0.2, 0.2}, {0.4, 0.4}}],
     TraditionalForm], "  ", ".", "  "}]}], 
  TraditionalForm]], "DisplayFormulaNumbered",

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubsuperscriptBox["\[Integral]", "P", "Q"], "  ", 
    RowBox[{"d", "\[VeryThinSpace]", 
     RowBox[{
      OverscriptBox["s", "\[RightVector]"], "  ", "\[CenterDot]", " ", 
      RowBox[{"\[Del]", "\[CurlyPhi]"}]}]}]}], " ", "=", "  ", 
   RowBox[{
    RowBox[{
     UnderscriptBox["\[Sum]", "i"], " ", 
     RowBox[{
      SubsuperscriptBox["\[Integral]", "P", "Q"], "  ", 
      RowBox[{"d", "\[VeryThinSpace]", 
       SubscriptBox["s", "i"], " ", 
       RowBox[{
        FractionBox["\[PartialD]", 
         RowBox[{"\[PartialD]", 
          SubscriptBox["x", "i"]}]], " ", "\[CurlyPhi]"}]}]}]}], " ", "=", 
    " ", 
    RowBox[{
     RowBox[{"\[CurlyPhi]", "(", "Q", ")"}], " ", "-", " ", 
     RowBox[{
